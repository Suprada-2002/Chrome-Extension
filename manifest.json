{
    "manifest_version": 2,
    "name": "Social Media Blocker",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "icons": { "48": "icon48.png", "128": "icon128.png" },
    "browser_action": {
        "default_popup": "Popup.html",
        "default_title": "Procrastination Blocker"
      }
}